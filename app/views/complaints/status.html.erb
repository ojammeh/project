<div class="container">
  <section>				
    <div id="container_demo" >
      <div id="wrapper">
        <div id="login" class="animate form">
        <%if(session[:userrole] == "admin")%>
            <%= link_to 'New Complaint', complaints_search_path, :style=>'color:#510269; font-style:italic; font-weight:bold; font-size:15px;' %>
           | <%= link_to 'Complaint Status', complaints_status_path, :style=>'color:#510269; font-style:italic; font-weight:bold; font-size:15px;' %>
           | <%= link_to 'Pending Dialout', complaints_pending_dialout_path, :style=>'color:#510269; font-style:italic; font-weight:bold; font-size:15px;' %>
           <%end%>
          <h1>Customer Care Complaints</h1> 
          <center>
          <div id="notsatisfied"></div>
            <table>
                <tr>
                  <th width="width: 5%">#</th>
                  <th width="width: 10%">MSISDN</th>
                  <th width="width: 10%">Complaint</th>
                  <th width="width: 10%">Feedback</th>
                  <th width="width: 5%">Time Launched</th>
                  <th width="width: 5%">Time Completed</th>
                  <th width="width: 5%">Customer Name</th>
                  <th width="width: 5%">Technician</th>
                </tr>
                <% @pending.each do |result|%>
                <tr>
                  <td><%=result.id%></td>
                  <td><%=result.anumber%><br/><% if result.status == 0 %><font color="red"><b>Pending...</b></font><%elsif result.status == 1%><font color="#00BA7F"><b>Complete</b><br/><%= link_to "Not Satisfied", {:action=> "not_satisfied", :remote => true, :controller => "complaints", :id => result.id} %></font><%end%><%if(result.status == 2 and session[:userrole] == "callcenter_dialout")%><font color="red"><b>Pending Dialout...</b></font><%end%></td>
                  <td><%=result.complaint%><% if result.status == 1 %><br/><%= link_to "Dismiss", {:action=> "dismiss", :controller => "complaints", :id => result.id} %><%end%><% if(result.status == 2 and session[:userrole] == "callcenter_dialout")%><br><%= link_to "Dismiss", {:action=> "dismiss_dialout", :controller => "complaints", :id => result.id} %><%end%></td>
                  <td><font color="red"><b><u><%=result.not_satisfied%></u></b></font> <br/><%=result.technical_comments%></td>
                  <td><%=result.time_in%></td>
                  <td><%=result.time_out%></td>
                  <td><%=result.customer_name%></td>
                  <td><%=result.technical_user%></td>
                </tr>
                <%end%>
            </table>
            
          </center>
              <p></p>                      
              <p class="change_link">
			         <font color="#7E17E4"><b>Africell Gambia BSU Department.</b> </font><font color="#556E70" size="2">All rights reserved. Â© Copyright 2015</font> <br>
              <font color="#556E70" size="2">Designed & Developed by <b>Mam M Cham</b></font>&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;
		        </p>
          </div>
        </div>
      </div>  
    </section>
 </div>