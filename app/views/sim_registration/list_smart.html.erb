<div class="container">
  <section>				
    <div id="container_demo" >
      <div id="wrapper">
        <div id="login" class="animate form">
            <h1>Sim Registration</h1> 
          <%= paginate @subs%>
            <table>
              <tr>
                <th>MSISDN</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Gender</th>
                <th>Nationality</th>
                <th>ID Type</th>
                
                <th>Date</th>
                <th>Agent</th>
                <th>Image</th>
                <th>Validate</th>
                <th>Invalidate</th>
              </tr>
              <%@subs.each do |sub|%>
              <tr>
                <td><%= sub.SIMNDC %><%= sub.SIMMSISDN %></td>
                <td><%=sub.FirstName%></td>
                <td><%=sub.lastName%></td>
                <td><%=sub.Gender%></td>
                <td><%=sub.Nationality%></td>
                <td><%=sub.IdType%></td>
                
                <td><%=sub.CreateDate%></td>
                <td><%=sub.CreateUser%></td>
                <!-- <td><%= link_to 'Image', {:controller => 'sim_registration', :action => 'view_image', :id => sub.id}, :method => :get%></td> -->
                <td><%= link_to 'Image', sim_registration_view_image_path(:id => sub.id),:onclick=>"window.open(this.href,'subscriber_details', 'height=650, width=800');return false;" %></td>
                <td><%=link_to "Validate", sim_registration_validate_smart_path(:id => sub.id) %></td>
                <td><%=link_to "Invalidate", sim_registration_invalidate_smart_path(:id => sub.id) %></td>
