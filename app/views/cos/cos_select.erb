<div class="container">
  <section>       
    <div id="container_demo" >
      <div id="wrapper">
        <div id="login" class="animate form">
          
          <form  action="/cos/check_reference" > 
            <h1>Change of SIM</h1> 
            <center><% if session[:ifregistered] == "true"%><b><%=flash[:first_name] %></b>&#8195; <b><%=flash[:id_type] %></b><br><%end%>
             <font color="red"> <%= flash[:postpaid] %> </font><font color="red"> <%= flash[:sameref] %> </font><%= flash[:success] %><br/> <%=@state%><br/> </center>
            <center>        
              <label for="msisdn">MSISDN</label>
              <input id="msisdn" size="8" name="msisdn" required="required" onkeypress="return isNumberKey(event)" maxlength="7" type="text" placeholder="MSISDN"  placeholder="MSISDN" />&#8195;

              <label for="ref1">Reference 1</label>
              <input id="ref1" size="8" name="ref1" required="required" onkeypress="return isNumberKey(event)" type="text" placeholder="Ref1"/>&#8195;

              <label for="ref2">Reference 2</label>
              <input id="ref2" size="8" name="ref2" required="required" onkeypress="return isNumberKey(event)" type="text" placeholder="Ref2"/> &#8195;

              <label for="ref3">Reference 3</label>
              <input id="ref3" size="8" name="ref3" required="required" onkeypress="return isNumberKey(event)" type="text" placeholder="Ref3"/>  
            </center>
            <p class="login button"> 
              <input type="submit" value="Check Reference" name="check_ref" /> 
            </p>
          </form>
              <p></p>                      
              <p class="change_link">
                 <font color="#7E17E4"><b>Africell Gambia BSU Department.</b> </font><font color="#556E70" size="2">All rights reserved. Â© Copyright 2015</font> <br>
              <font color="#556E70" size="2">Designed & Developed by <b>Mam M Cham</b></font>&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;&#8195;
            </p>
          </div>
        </div>
      </div>  
    </section>
 </div>