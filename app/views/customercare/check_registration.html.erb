<div class="container">
  <section>				
    <div id="container_demo" >
      <div id="wrapper">
        <div id="login" class="animate form">
          
          <form  action="/customercare/add_details" method="post" > 
            <h1>Value Added Services</h1> 
            <center>             
              <label for="msisdn" > MSISDN: </label>
              <input id="msisdn" size="8" name="msisdn" required="required" readonly onkeypress="return isNumberKey(event)" maxlength="7" type="text" value="<%=session[:msisdn]%>"/>
              &#8195;&#8195;
              <label for="f_name" > First Name: </label>
              <input id="f_name" size="15" name="f_name" required="required" type="text" placeholder="First Name"/>&#8195;&#8195;

              <label for="l_name" > Last Name: </label>
              <input id="l_name" size="15" name="l_name" required="required" type="text" placeholder="Family Name"/>&#8195;&#8195;<br/><br/>

              <label for="id_type">ID Type: </label>
                <select name ="id_type">
                  <option value="0" disabled selected>ID Type</option>
                  <option value="biometric">Biometric ID</option>
                  <option value="drivers_license">Driver's License</option>
                  <option value="voter_card">Voter's Card</option>
                  <option value="passport">National Passport</option>
                  <option value="residence_permit">Residence Permit</option>
                  <option value="foreign_passport">Foreign Passport</option>
                  <option value="foreign_id_card">Foreign ID Card</option>
                  <option value="alien_card">Alien Card</option>
                  <option value="non_gam_biometric_id">Non-Gambian Biometric ID</option> 
                </select>&#8195;&#8195;

              <label for="id_number" > ID Number: </label>
              <input id="id_number" size="15" name="id_number" type="text" placeholder="ID Number"/>

            </center>
            <p class="login button"> 
              <input type="submit" value="Submit" name="check_vas" /> 
						</p>
          </form>
              <p></p>                      
              <p class="change_link">
