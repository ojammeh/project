<title>Add Item</title>
<h1>Add Item</h1>
	<form action="/stocks/new_item" method="post" >
		<div class="row">
			<div class="col-md-6 col-md-offset-3">
				
				<label>Date</label>
				<input type="date" name="date" id="datepicker" class="form-control" placeholder="Date" required />
				<label>Part Number</label>
				<input type="text" name="part_number" class="form-control" placeholder="Part Number" required />
				<label>Description</label>
					<input type="textarea" name="description" class="form-control" placeholder="Description" required />
				<label>Site</label>
					<input type="text" name="site" class="form-control" placeholder="Site" required />
				<label>IN</label>
					<input type="text" name="in" class="form-control" placeholder="IN" required />
				<label>OUT</label>
					<input type="text" name="out"   class="form-control" placeholder="OUT" required />

				<input type="submit" name="submit" id="submit" value="Save" class="btn btn-primary" />
			</div>
		</div>							
	</form>